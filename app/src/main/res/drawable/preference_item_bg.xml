<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item
		android:drawable="@drawable/list_item_overstepped_light"
		android:top="-1px" />
	<item>
		<selector android:variablePadding="false">
			<item
				android:drawable="@drawable/preference_first_item_bg_normal"
				android:state_enabled="false" />
			<item
				android:drawable="@drawable/preference_first_item_bg_pressed"
				android:state_pressed="true" />
			<item android:drawable="@drawable/preference_first_item_bg_normal" />
			<item
				android:drawable="@drawable/preference_middle_item_bg_normal"
				android:state_enabled="false" />
			<item
				android:drawable="@drawable/preference_middle_item_bg_pressed"
				android:state_pressed="true" />
			<item android:drawable="@drawable/preference_middle_item_bg_normal" />
			<item
				android:drawable="@drawable/preference_last_item_bg_normal"
				android:state_enabled="false" />
			<item
				android:drawable="@drawable/preference_last_item_bg_pressed"
				android:state_pressed="true" />
			<item android:drawable="@drawable/preference_last_item_bg_normal" />
			<item
				android:drawable="@drawable/preference_single_item_bg_normal"
				android:state_enabled="false" />
			<item
				android:drawable="@drawable/preference_single_item_bg_pressed"
				android:state_pressed="true" />
			<item android:drawable="@drawable/preference_single_item_bg_normal" />
		</selector>
	</item>
</layer-list>
