<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
	android:paddingLeft="@dimen/preference_item_padding_left"
	android:paddingTop="@dimen/preference_category_gap_height"
	android:paddingRight="@dimen/preference_item_padding_right">
	<item>
		<shape android:shape="rectangle">
			<solid android:color="@android:color/transparent" />
			<size android:height="@dimen/preference_category_height" />
		</shape>
	</item>
	<item
		android:gravity="top"
		android:top="@dimen/preference_category_gap_height">
		<shape android:shape="rectangle">
			<solid android:color="@android:color/white" />
		</shape>
	</item>
	<item
		android:drawable="@drawable/preference_first_last_divider_line_light"
		android:gravity="top"
		android:top="@dimen/preference_category_gap_height" />
	<item
		android:drawable="@drawable/divider_line_light"
		android:gravity="bottom"
		android:left="@dimen/preference_item_padding_left" />
</layer-list>
